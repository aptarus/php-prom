{
    "name": "aptarus/php_prom",
    "type": "library",
    "description": "Prometheus client library for PHP.",
    "keywords": [
        "aptarus",
        "prometheus",
        "php_prom"
    ],
    "homepage": "https://github.com/aptarus/php_prom",
    "license": "MIT",
    "authors": [
        {
            "name": "Kevin Lyda",
            "email": "kevin.lyda@aptarus.com",
            "homepage": "https://www.aptarus.com/",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~5.5|~7.0",
        "fire015/flintstone": "2.*"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "aptarus\\php_prom\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "aptarus\\php_prom\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
